# LiveKit
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# Model paths (YOLO11 - latest)
YOLO_MODEL_PATH=weights/yolo11n.pt
CLASSIFIER_MODEL_PATH=weights/classifier.json

# Processing
PROCESS_FPS=10
FRAME_WIDTH=640
FRAME_HEIGHT=480

# GPU
GPU_MEMORY_FRACTION=0.8
USE_GPU=true

# Detection thresholds
YOLO_CONFIDENCE=0.5
FACE_CONFIDENCE=0.5
CHEATING_THRESHOLD=0.7

# Alert settings
ALERT_COOLDOWN=5
SLIDING_WINDOW=60

# Database (optional)
# MONGODB_URI=mongodb://localhost:27017/proctoring
